# Environment variables template
# Copy this file to .env and fill in your values

# Django settings
SECRET_KEY=your-super-secret-key-here-change-this
DEBUG=False

# Database (Cloud SQL PostgreSQL connection string)
DATABASE_URL=postgresql://username:password@/database_name?host=/cloudsql/your-project:region:instance-name

# Frontend URL (Cloud Storage bucket URL or Cloud Run frontend URL)
FRONTEND_URL=https://your-frontend-domain.com

# Google Cloud Project settings
GOOGLE_CLOUD_PROJECT=your-project-id
GOOGLE_CLOUD_REGION=us-central1

# Cloud SQL settings
CLOUD_SQL_CONNECTION_NAME=your-project:region:instance-name